{
    "manifest_version": 3,
    "name": "Gemini AI Assistant",
    "version": "1.0",
    "description": "An extension to interact with the Gemini AI.",
    "action": {
        "default_popup": "popup.html"
       
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "storage"]
}
